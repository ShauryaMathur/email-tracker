{
  "manifest_version": 3,
  "name": "Gmail UUID Tracker Injector",
  "version": "1.0",
  "description": "Injects UUID into tracking pixel src in Gmail compose",
  "permissions": [],
  "host_permissions": [
    "https://mail.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
